{
  "name": "awesome-task-manager",
  "version": "0.0.0",
  "private": true,
  "description": "awesome task manager",
  "license": "UNLICENSED",
  "author": "Afshin Jalili",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "eslint:check": "eslint \"{src,apps,libs,test}/**/*.ts\"",
    "eslint:fix": "npm run eslint:check -- --fix",
    "lint": "npm run eslint:check && npm run prettier:check",
    "precommit": "npm run lint",
    "prepare": "husky install",
    "prepush": "npm run test",
    "prettier:check": "prettier --check ./**/*.{ts,js,json,*rc}",
    "prettier:write": "prettier --write ./**/*.{ts,js,json,*rc}",
    "start": "nest start",
    "start:debug": "nest start --debug --watch",
    "start:dev": "nest start --watch",
    "start:prod": "node dist/apps/nashville/main",
    "test": "jest",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./apps/nest-boilerplate/test/jest-e2e.json",
    "test:watch": "jest --watch"
  },
  "dependencies": {
    "@grpc/grpc-js": "1.9.0",
    "@grpc/proto-loader": "0.7.8",
    "@nestjs/common": "10.1.3",
    "@nestjs/config": "3.0.0",
    "@nestjs/core": "10.1.3",
    "@nestjs/mapped-types": "*",
    "@nestjs/microservices": "10.1.3",
    "@nestjs/platform-express": "10.1.3",
    "@nestjs/swagger": "7.1.6",
    "@nestjs/typeorm": "10.0.0",
    "class-transformer": "0.5.1",
    "class-validator": "0.14.0",
    "js-yaml": "4.1.0",
    "nestjs-grpc-exceptions": "0.2.1",
    "pg": "8.11.2",
    "reflect-metadata": "0.1.13",
    "rimraf": "5.0.1",
    "rxjs": "7.8.1",
    "typeorm": "0.3.17",
    "uuid": "9.0.0"
  },
  "devDependencies": {
    "@nestjs/cli": "10.1.11",
    "@nestjs/schematics": "10.0.1",
    "@nestjs/testing": "10.1.3",
    "@types/express": "4.17.17",
    "@types/jest": "29.5.3",
    "@types/node": "20.4.9",
    "@types/supertest": "2.0.12",
    "@types/uuid": "9.0.2",
    "@typescript-eslint/eslint-plugin": "5.62.0",
    "@typescript-eslint/parser": "5.62.0",
    "commitizen": "4.3.0",
    "eslint": "8.46.0",
    "eslint-config-prettier": "8.9.0",
    "eslint-plugin-import": "2.28.0",
    "husky": "8.0.3",
    "jest": "29.6.2",
    "prettier": "2.8.8",
    "supertest": "6.3.3",
    "ts-jest": "29.1.1",
    "ts-loader": "9.4.4",
    "ts-node": "10.9.1",
    "ts-proto": "1.156.2",
    "tsconfig-paths": "4.2.0",
    "typescript": "5.1.6"
  },
  "optionalDependencies": {
    "fsevents": "2.3.2"
  },
  "engines": {
    "node": "^16.14.0 || ^18.10.0 || ^20.0.0",
    "npm": ">=8.0.0"
  }
}
